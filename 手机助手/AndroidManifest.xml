<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phonehelper"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.zh.phonehelp.BootActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zh.phonehelp.AnimActivity"
            android:label="动画" >
        </activity>
        <activity
            android:name="com.zh.phonehelp.MenuActivity"
            android:label="主界面"
            android:launchMode="singleTask" >
        </activity>
        <activity
            android:name="com.zh.phonehelp.SjjsActivity"
            android:label="手机加速" >
        </activity>
        <activity
            android:name="com.zh.phonehelp.RjglActivity"
            android:label="软件管理" >
        </activity>
        <activity
            android:name="com.zh.phonehelp.SjjcActivity"
            android:label="手机检测" >
        </activity>
        <activity
            android:name="com.zh.phonehelp.TxdqActivity"
            android:label="通讯大全"
            android:launchMode="singleTask" >
        </activity>
        <activity
            android:name="com.zh.phonehelp.LjqlActivity"
            android:label="垃圾清理" >
        </activity>
        <activity
            android:name="com.zh.phonehelp.TxdqlistActivity"
            android:label="@string/title_activity_txdqlist" >
        </activity>
        <activity
            android:name="com.zh.phonehelp.SoftcontentActivity"
            android:label="@string/title_activity_softcontent" >
        </activity>
    </application>

</manifest>